<?xml version="1.0" encoding="utf-8"?>

<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pages="clr-namespace:endproject.Pages"
             xmlns:main="clr-namespace:endproject.Pages.Main"
             x:Class="endproject.Pages.MainShell"
             Shell.FlyoutBehavior="Disabled">
    <TabBar>
        <Tab Title="List">
            <ShellContent ContentTemplate="{DataTemplate main:MainPage}" />
        </Tab>
        <Tab Title="Users">
            <ShellContent ContentTemplate="{DataTemplate main:Users}" />
        </Tab>
    </TabBar>
</Shell>
